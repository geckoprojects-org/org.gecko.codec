{
  ":configurator:resource-version": 1,
  
  "MongoRF": 
  {
    "database": "text",
    "host": "localhost",
    "jsonFactory.target":"(codecType=MONGO)",
    "module.target":"(codecType=MONGO)"
	},
	"CodecModule~test":
	{
		"codecType" : "MONGO",
		//"modelInfo.target": "(&(emf.name=person)(emf.name=address))",
		"useId" : true,
		"idOnTop" : false,
		"dateFormat": "yyyy-MM-dd'T'HH:mm:ss",
		"timeZone" : "Europe/Berlin",
		"locale" : "en-US",
		"indentOutput": true
	},
	"CodecFactoryConfigurator": {
		"codecType" : "json",
		"enableFeatures": [
			"JsonReadFeature.ALLOW_JAVA_COMMENTS"
		],
		"disableFeatures" : []
	}
}