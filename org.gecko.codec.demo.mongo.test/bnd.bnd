Test-Cases: ${classes;HIERARCHY_INDIRECTLY_ANNOTATED;org.junit.platform.commons.annotation.Testable;CONCRETE}

-library: enableOSGi-Test, enable-emf


# sets the usually required buildpath using the bnd library, you can extend it with the normal -buildpath to your liking
-buildpath: \
	org.osgi.framework;version=latest,\
	org.osgi.service.cm;version=latest,\
	org.osgi.test.junit5.cm;version=latest,\
	org.mongodb.mongo-java-driver;version=latest,\
	org.gecko.emf.osgi.api;version=latest,\
	org.gecko.emf.collections,\
	org.gecko.mongo.osgi.api;version=latest,\
	org.gecko.mongo.osgi.component;version=latest,\
	org.gecko.emf.mongo.component;version=latest,\
	org.gecko.emf.mongo.pushstream;version=latest,\
	org.osgi.util.promise,\
	org.osgi.util.pushstream,\
	org.osgi.service.component,\
	org.gecko.code.demo.model;version=latest,\
	org.gecko.codec.demo,\
	org.gecko.codec.info,\
	com.fasterxml.jackson.core.jackson-databind,\
	com.fasterxml.jackson.core.jackson-core
	
Private-Package: org.gecko.codec.demo.mongo.test

Bundle-Version: 1.0.0.SNAPSHOT

# Sometimes without DynamicImport Mockito will not find certain classes
#DynamicImport-Package: *